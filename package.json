{
	"devDependencies": {
		"@babel/cli": "^7.24.7",
		"@babel/core": "^7.24.7",
		"@babel/preset-env": "^7.24.7",
		"@babel/preset-react": "^7.24.7",
		"nib": "^1.2.0",
		"stylus": "^0.63.0"
	},
	"scripts": {
		"stylus": "stylus src/index.styl -o css/bundle.css -w -m -r --include-css",
		"babel": "babel src/modules -d js -w",
		"build": "rm -rf js css & babel src/modules -d js --minified & stylus src/index.styl -o css/bundle.css -c -r --include-css"
	}
}
